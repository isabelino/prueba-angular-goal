    "bootstrap": "^5.3.0",
    "datatables.net": "^1.11.3",
    "datatables.net-dt": "^1.11.3",
    "firebase": "^10.0.0",
    "jest": "^29.6.1",
    "jest-preset-angular": "^13.1.1",
